{
  "name": "comuse-core",
  "version": "4.1.1",
  "description": "A library of commonly used utility functions",
  "author": "ajiu9 <615944323@qq.com> (https://github.com/ajiu9/)",
  "license": "MIT",
  "homepage": "https://github.com/ajiu9/comuse",
  "repository": {
    "type": "git",
    "url": "https://github.com/ajiu9/comuse",
    "directory": "packages/comuse"
  },
  "keywords": [
    "vue",
    "javaScript",
    "util"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./index.mjs",
    "./*": "./*",
    "./metadata": "./metadata.mjs"
  },
  "main": "./index.mjs",
  "module": "./index.mjs",
  "unpkg": "./index.iife.min.js",
  "jsdelivr": "./index.iife.min.js",
  "types": "./index.d.mts",
  "files": [
    "*.d.mts",
    "*.js",
    "*.mjs"
  ],
  "scripts": {
    "build": "rollup --config=rollup.config.ts --configPlugin=rollup-plugin-esbuild",
    "prepack": "pnpm run build"
  },
  "peerDependencies": {
    "vue": "^3.5.0"
  },
  "dependencies": {
    "comuse-metadata": "workspace:*",
    "comuse-shared": "workspace:*"
  },
  "buildOptions": {
    "name": "CuseCore",
    "formats": [
      "esm-bundler",
      "cjs",
      "global",
      "dts"
    ]
  }
}
