{
  "name": "comuse",
  "type": "module",
  "version": "3.2.6",
  "private": true,
  "packageManager": "pnpm@9.3.0",
  "description": "A library of commonly used utility functions",
  "author": "ajiu9 <615944323@qq.com> (https://github.com/ajiu9/)",
  "license": "MIT",
  "keywords": [
    "javaScript",
    "util"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=16.11.0"
  },
  "scripts": {
    "build": "pnpm run update && node scripts/build.js",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "release": "pnpm build && bumpp -r && pnpm -r publish --access public",
    "clean": "rimraf packages/*/dist",
    "docs": "pnpm run update && vitepress dev packages --open",
    "docs:build": "pnpm run update && vitepress build packages",
    "docs:preview": "pnpm run update && vitepress preview packages",
    "prepare": "simple-git-hooks",
    "test": "pnpm run test:unit",
    "test:cov": "vitest run --coverage",
    "test:browser": "vitest --project browser --workspace=vitest.workspace.ts",
    "test:unit": "vitest --project unit --workspace=vitest.workspace.ts",
    "update": "pnpm run -C packages/metadata update"
  },
  "dependencies": {
    "typing-ts": "^0.0.4"
  },
  "devDependencies": {
    "@ajiu9/eslint-config": "^2.5.2",
    "@iconify/json": "^2.2.313",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@shikijs/vitepress-twoslash": "^1.29.2",
    "@types/node": "^22.13.9",
    "@vitest/coverage-v8": "2.1.5",
    "bumpp": "^9.11.1",
    "comuse-core": "workspace:*",
    "comuse-metadata": "workspace:*",
    "comuse-shared": "workspace:*",
    "esbuild": "^0.19.12",
    "eslint": "^9.21.0",
    "execa": "^7.2.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "gray-matter": "^4.0.3",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.4.3",
    "md5": "^2.3.0",
    "minimist": "^1.2.8",
    "rollup": "^4.34.9",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "simple-git": "^3.27.0",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "unocss": "^0.62.4",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^0.27.5",
    "vite": "^5.4.14",
    "vite-plugin-inspect": "^0.8.9",
    "vitepress": "^1.6.3",
    "vitest": "^2.1.9",
    "vue": "^3.5.13"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
